    "#add files to bucket \n",
    "import boto3\n",
    "import os\n",
    "\n",
    "client = boto3.client('s3')\n",
    "bucket = os.environ.get('DB_BUCKET') #retrieve bucket name from Environment Variables\n",
    "\n",
    "client.upload_file('cute_cat.jpg', bucket, 'cat.jpg') #image files will be added to bucket\n",
    " "
